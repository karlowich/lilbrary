{
	"name": "lilbrary",
	"productName": "LilBrary",
	"version": "1.0.0",
	"description": "Library app",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"package-mac": "electron-packager . --overwrite --asar --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds --app-copyright=LilBrary2019",
		"package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --app-copyright=LilBrary2019",
		"package-linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds --app-copyright=LilBrary2019"
	},
	"author": "Karl Bonde Torp",
	"license": "MIT",
	"devDependencies": {
		"electron": "6.0.12",
		"electron-packager": "^14.1.0"
	},
	"dependencies": {
		"electron-store": "^5.0.0"
	}
}
